{
  "name": "bookmark-rag-extension",
  "version": "4.0.1",
  "description": "Capture and semantically search your bookmarks using RAG",
  "type": "module",
  "scripts": {
    "dev": "npx vite --config vite.config.chrome.ts",
    "dev:chrome": "npx vite --config vite.config.chrome.ts",
    "dev:firefox": "npx vite --config vite.config.firefox.ts",
    "dev:web": "npx vite --config vite.config.web.ts",
    "build": "npm run build:chrome && npm run build:firefox",
    "build:web": "npx vite build --config vite.config.web.ts",
    "build:chrome": "npx vite build --config vite.config.chrome.ts",
    "build:firefox": "npx vite build --config vite.config.firefox.ts",
    "preview": "npx vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "check": "npm run typecheck & npm run lint & wait",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e:chrome": "npx tsx tests/e2e.test.ts",
    "test:e2e:firefox": "npx tsx tests/e2e-firefox.test.ts",
    "test:e2e:web": "npx tsx tests/e2e-web.test.ts",
    "test:all": "npm run test:unit && npm run test:e2e:chrome && npm run test:e2e:web",
    "screenshots": "npx tsx scripts/capture-screenshots.ts"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "dexie": "^4.2.1",
    "marked": "^17.0.1",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.3.0",
    "@eslint/js": "^9.39.2",
    "@types/chrome": "^0.1.32",
    "@types/turndown": "^5.0.6",
    "eslint": "^9.39.2",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "puppeteer-core": "^24.33.0",
    "selenium-webdriver": "^4.39.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
